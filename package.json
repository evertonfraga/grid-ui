{
  "name": "grid-ui",
  "version": "0.1.30",
  "private": true,
  "main": "./app/shell-electron.js",
  "homepage": "./",
  "repository": {
    "type": "git",
    "url": "https://github.com/ethereum/grid-ui.git"
  },
  "scripts": {
    "analyze-modules": "source-map-explorer build/static/js/1.*",
    "analyze": "source-map-explorer build/static/js/main.*",
    "start": "cross-env PORT=3080 CI=false BROWSER=none react-scripts start",
    "_build": "react-scripts build",
    "build": "react-scripts-ext build app",
    "package": "react-scripts-ext package",
    "deploy": "react-scripts-ext deploy",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "lint:watch": "esw -w src --ext .jsx --ext .js",
    "lint": "eslint src --ext .jsx --ext .js",
    "dev:tools": "remotedev & (sleep 3 && open http://localhost:8000)"
  },
  "dependencies": {
    "@material-ui/core": "^3.9.2",
    "@material-ui/icons": "^3.0.2",
    "ansi-to-react": "^5.0.0",
    "bignumber.js": "^7.2.1",
    "bn.js": "^4.11.8",
    "classnames": "^2.2.6",
    "codemirror": "^5.48.0",
    "ethereum-react-components": "^1.13.11",
    "lodash": "^4.17.11",
    "moment": "^2.24.0",
    "numeral": "^2.0.6",
    "prop-types": "^15.6.2",
    "react": "^16.4.1",
    "react-codemirror2": "^6.0.0",
    "react-dom": "^16.4.1",
    "react-md-spinner": "^0.3.0",
    "react-minimal-pie-chart": "^3.5.0",
    "react-redux": "^5.0.7",
    "react-scripts": "^2.1.1",
    "redux": "^4.0.0",
    "redux-persist": "^5.10.0",
    "redux-thunk": "^2.3.0",
    "remote-redux-devtools": "^0.5.16",
    "semver": "^5.6.0",
    "styled-components": "^4.1.3",
    "web3": "^1.0.0-beta.36",
    "xterm": "^3.14.5"
  },
  "ignore": {},
  "devDependencies": {
    "babel-register": "^6.26.0",
    "colors": "^1.3.0",
    "create-react-app-extensions": "^1.1.1",
    "cross-env": "^5.2.0",
    "dotenv": "^6.0.0",
    "electron": "^2.0.8",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-watch": "^4.0.2",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.1",
    "prettier": "^1.16.1",
    "react-test-renderer": "^16.5.2",
    "remotedev-server": "^0.3.1",
    "source-map-explorer": "^1.6.0",
    "webpack-bundle-analyzer": "^3.0.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,json,css,md}": [
      "prettier --no-semi --single-quote --write",
      "git add"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
